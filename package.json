    "@polymer/app-layout": "^3.0.0-pre.25",
    "@polymer/app-route": "^3.0.2",
    "@polymer/decorators": "^3.0.0",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-media-query": "^3.0.1",
    "@polymer/polymer": "^3.0.5",
    "@vaadin/vaadin-button": "^2.1.0",
    "@vaadin/vaadin-combo-box": "^4.2.0-alpha3",
    "@vaadin/vaadin-dialog": "^2.2.0-alpha2",
    "@vaadin/vaadin-material-styles": "^1.2.0",
    "@vaadin/vaadin-notification": "^1.2.0-alpha2",
    "@vaadin/vaadin-progress-bar": "^1.1.0",
    "@webcomponents/webcomponentsjs": "^2.1.1",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "browser-sync": "^2.24.7",
    "clang-format": "^1.2.4",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "prpl-server": "^1.2.0",
    "tslint": "5.11.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "start": "gulp",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "tsc && polymer build --auto-base-path && gulp prpl-server",
    "build:static": "tsc && polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000",
    "generate-typings": "gen-typescript-declarations --root  src/types --outDir  src/types",
    "deploy:production": "gcloud app deploy server/app.yaml --project marketing-leavitt-com",
    "deploy:development": "gcloud app deploy server/app.yaml --project dev-marketing-leavitt-com"
  }
}
